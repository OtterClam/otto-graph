{
  "name": "otto-subgraph",
  "version": "0.1.0",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "auth": "graph auth --product hosted-service",
    "deploy": "yarn build && graph deploy --product hosted-service otterclam/otto",
    "deploy:testnet": "yarn build && graph deploy --product hosted-service otterclam/otto_testnet",
    "create-local": "graph create --node http://localhost:8020/ otterclam/otto",
    "remove-local": "graph remove --node http://localhost:8020/ otterclam/otto",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 otterclam/otto"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.23.1",
    "@graphprotocol/graph-ts": "0.23.1"
  }
}
